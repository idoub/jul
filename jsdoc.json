{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "templates": {
        "cleverLinks": false,
        "collapseSymbols": false,
        "copyright": "Isaac J Doub Â© 2017",
        "dateFormat": "ddd MMM Do YYYY",
        "linenums": true,
        "methodHeadingReturns": false,
        "monospaceLinks": true,
        "outputSourceFiles": true,
        "outputSourcePath": true,
        "protocol": "html://"
    },
    "opts": {
        "destination": "public",
        "template": "node_modules/minami"
    },
    "plugins": [
        "plugins/markdown"
    ]
}